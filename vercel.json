{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.go",
            "use": "@vercel/go"
        },
        {
            "src": "terminal/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/terminal/$1"
        }
    ]
}